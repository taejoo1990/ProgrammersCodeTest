package myProejct;

import java.util.ArrayList;

public class Main19 {
	public static void main(String[] args) {
		Solution19 sol = new Solution19();
	}
}

class Solution19 {
    public int solution(int[] num_list) {
        int answer = 0;
        for(int tmp : num_list) {
        	while(true) {
        		if(tmp==1) {
        			break;
        		}
        		answer ++;
        		tmp = tmp%2 ==0 ? tmp/2 : (tmp-1)/2;	
        	}
        }
        return answer;
    }
}