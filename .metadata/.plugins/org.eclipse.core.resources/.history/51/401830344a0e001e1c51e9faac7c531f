package myProejct;

import java.util.Arrays;

public class Main9 {
	public static void main(String[] args) {
		Solution9 sol = new Solution9();
		int[] case1 = {1, 2, 1, 4, 5, 2, 9};
	}
}

class Solution9 {
    public int[] solution(int[] arr, int[] query) {
        int index = 0;
        System.out.println(Arrays.toString(arr));
        for(int tmp : query) {
        	if(index%2==0) {
        		arr = Arrays.copyOfRange(arr, 0, tmp);
        		System.out.println(Arrays.toString(arr));
        	}else {
        		arr = Arrays.copyOfRange(arr, tmp, arr.length-1);
        		System.out.println(Arrays.toString(arr));
        	}
        	index ++;
        }
        return arr;
    }
}