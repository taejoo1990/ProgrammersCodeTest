package myProejct;

import java.util.ArrayList;

public class Level2 {
	public static void main(String[] args) {
		Solution2 sol = new Solution2();
		sol.solution("JAAN");
	}
}

class Solution2 {
    public int solution(String name) {
    	int minCnt = 0;
    	for(char c : name.toCharArray()) {
    		minCnt +=calMin(c);
    	}
    	minCnt += name.length()-1;
    	if(name.indexOf("A")==1) {
    		int moveCnt = 0;
    		for(int i=1; i<=name.length();i++) {
    			if(name.charAt(i) == 'A') {
    				moveCnt++;
    				continue;
    			}else {
    				break;
    			}		
    		}
    		minCnt-=moveCnt;
    	}
    	System.out.println(minCnt);
    	return minCnt;
    }
    private int calMin(char ch) {
    	int order = (int)ch-65;
    	int reverse = 90-(int)ch;
    	return order < reverse ? order : reverse+1;
    }
}
