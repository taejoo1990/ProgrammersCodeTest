class Solution6{
    public String solution(String my_string, int[] indices) {
        String answer = "";
        for(int tmp : indices) {
        	String pre = answer.substring(0,tmp-1);
        	String suf = answer.substring(tmp);
        	answer = pre+suf;
        }
        System.out.println(answer);
        return answer;
    }
}

