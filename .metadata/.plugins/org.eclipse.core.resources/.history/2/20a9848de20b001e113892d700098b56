class Solution6{
    public String solution(String my_string, int[] indices) {
        String answer = "";
        for(int tmp : indices) {
        	String pre = my_string.substring(0,tmp);
        	String suf = my_string.substring(tmp);
        	answer = pre+suf;
        }
        System.out.println(answer);
        return answer;
    }
}

